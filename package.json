{
  "name": "nft-blocks",
  "description": "A tool to create virtual tradeable destinations",
  "author": "Daniel Esteban Nombela",
  "version": "1.0.0",
  "license": "MIT",
  "private": true,
  "scripts": {
    "client:build": "rollup -c client/rollup.config.js",
    "client:watch": "npm run client:build -- -w",
    "client:serve": "static -c 1 client",
    "start": "npm-run-all --parallel client:watch client:serve"
  },
  "dependencies": {
    "@rollup/plugin-json": "^4.1.0",
    "@rollup/plugin-node-resolve": "^8.4.0",
    "rollup": "^2.23.0",
    "rollup-plugin-livereload": "^1.3.0",
    "rollup-plugin-svelte": "^5.2.3",
    "rollup-plugin-terser": "^6.1.0",
    "svelte": "^3.24.0",
    "three": "^0.119.1"
  },
  "devDependencies": {
    "eslint": "^7.5.0",
    "eslint-config-airbnb-base": "^14.2.0",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-svelte3": "^2.7.3",
    "node-static": "^0.7.11",
    "npm-run-all": "^4.1.5"
  }
}
